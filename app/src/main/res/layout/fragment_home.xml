    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <variable
            name="vm"
            type="com.blink22.sample.topKotlins.ui.views.home.HomeViewModel" />
    </data>

    <androidx.recyclerview.widget.RecyclerView
